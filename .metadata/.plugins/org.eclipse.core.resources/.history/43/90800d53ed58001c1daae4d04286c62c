package oneExe;

import java.util.Scanner;

public class Program {
public static void main(String[]args) {
	Scanner sc = new Scanner(System.in);
	int n = sc.nextInt();
	int[][] mat = new int[n][n];
	
	for(int i=0;i<mat.length;i++) {
		System.out.println(i);
	}
	
	//System.out.println(mat);
	sc.close();
}
}
