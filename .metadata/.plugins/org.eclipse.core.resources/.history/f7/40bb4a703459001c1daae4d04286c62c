package oneExe;

import java.util.Scanner;

public class Program {
public static void main(String[]args) {
	Scanner sc = new Scanner(System.in);
	int n = sc.nextInt();
	int[][] mat = new int[n][n];
	
	for(int i=0;i<mat.length;i++) {
		mat[i][i] = sc.nextInt();
		for(int j=0;j<mat.length;j++) {
			mat[j][j] = sc.nextInt();
		}
	}
	
	//System.out.println(mat);
	sc.close();
}
}
